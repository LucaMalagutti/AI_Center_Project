{
    "pipeline": {
    "dataset": "wn18rr",
    "random_seed" : 40,
    "model": "MuRE",
    "model_kwargs": {
      "embedding_dim": 200,
      "p": 2,
      "power_norm": true,
      "entity_initializer": "normal_",
      "entity_initializer_kwargs": {
        "std": 1.0e-03
      },
      "entity_bias_initializer": "zeros_",
      "relation_initializer": "normal_",
      "relation_initializer_kwargs": {
        "std": 1.0e-03
      },
      "relation_matrix_initializer": "uniform_",
      "relation_matrix_initializer_kwargs": {
        "a": -1,
        "b": 1
      }
    },
    "optimizer": "SGD",
    "optimizer_kwargs": {
      "lr": 50.0
    },
    "loss": "BCEWithLogitsLoss",
    "loss_kwargs": {
      "reduction": "mean"
    },
    "training_loop": "SLCWA",
    "negative_sampler": "basic",
    "negative_sampler_kwargs": {
      "num_negs_per_pos": 50
    },
    "training_kwargs": {
      "num_epochs": 500,
      "batch_size": 128
    },
    "evaluator_kwargs": {
      "filtered": true
    }
  }
}